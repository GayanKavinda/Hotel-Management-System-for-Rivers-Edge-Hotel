<script>
            function addreservationz() {


                var yewxmlhttp;
                if (window.XMLHttpRequest) {

                    yewxmlhttp = new XMLHttpRequest();


                } else {
                    yewxmlhttp = new ActiveXObject("Microsoft.XMLHTTP");

                }

                yewxmlhttp.onreadystatechange = function () {


                    if (yewxmlhttp.readyState == 4 && yewxmlhttp.status == 200) {

                        var gett = yewxmlhttp.responseText;
                        if (gett === "ok") {
                            //   alert("inside if");
                            swal("Done!", "Booking Added Sucessfully..!", "success");
                            window.location = "index.jsp";

                        } else if (gett == "userz") {
                            
                            swal("Invalid Entry!", "Please Login TO COntinue", "error");
                            window.location = "login.jsp";
                        }


                    }
                }

                var tablecount = document.getElementById("nosseat").value;
                var occasionz = document.getElementById("occasionz").value;
                var mealplans = document.getElementById("mealzplan").value;
                var selecteddate = document.getElementById("selecteddatez").value;
                var selectetime = document.getElementById("selectedtime").value;
                alert(tablecount);
                alert(occasionz);
                alert(mealplans);
                alert(selecteddate);
                alert(selectetime);


                if (tablecount == "" || occasionz == "" || mealplans == "" || selecteddate == "" || selectetime == "") {

                    swal("Invalid Entry!", "Please Fill Neccessery Feilds", "error");


                } else if (occasionz == "Occasion" || mealplans == "MealPlan") {

                    swal("Invalid Entry!", "Please Fill Neccessery Feilds", "error");

                } else {
                    yewxmlhttp.open("POST", "addreservationz", true);
                    yewxmlhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
                    yewxmlhttp.send("tablecoun=" + tablecount + "&occacianzz=" + occasionz + "&mealplanz=" + mealplans + "&selecteddate=" + selecteddate+ "&selectedtimez=" + selectetime);

                }

                // alert(cusreveiw);




            }
